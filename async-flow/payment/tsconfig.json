{
    "compilerOptions": {
        "target": "es2016",
        "module": "CommonJS",
        "rootDir": "./",
        "moduleResolution": "node10",
        "allowJs": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
